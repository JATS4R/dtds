{
    "version": 2,
    "name": "jats-dtds",
    "alias": "jats-dtds.now.sh",
    "builds": [
        { "src": "./schema/**", "use": "@now/static" },
        { "src": "./catalog.xml", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/schema/(.*)", "dest": "schema/$1" },
        { "src": "/catalog.xml", "dest": "/catalog.xml" }
    ]
}
